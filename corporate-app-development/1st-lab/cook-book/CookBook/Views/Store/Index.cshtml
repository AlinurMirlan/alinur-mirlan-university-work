@model IEnumerable<Dish>
@{
    ViewData["Title"] = "Cook Book Store";
    ViewData["HeaderTitle"] = "Dishes";
}

<section>
    <div class="bg-green-100 hover:bg-green-200">
        <a href="#" class="block text-xl text-center py-5 text-slate-600">You ordered # dishes</a>
    </div>
    <div class="flex flex-wrap items-center">
        @foreach (Dish dish in Model)
        {
            <partial name="_DishStoreCard" model="dish"/>
        }
    </div>
</section>

